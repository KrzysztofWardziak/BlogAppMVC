    <script src="~/Scripts/js/photograph-main.js"></script>
    <script src="~/Scripts/sticky/jquery.sticky.min.js"></script>
    <script src="~/Scripts/sticky/sticky-settings.js"></script>
    <script src="~/Scripts/js/fancybox/fancybox-settings.js"></script>
    <script src="~/Scripts/js/fancybox/jquery.fancybox.js"></script>
    <script src="~/Scripts/js/isotope-setting.js"></script>
    <script src="~/Scripts/js/source/flexslider-setting.js"></script>
    <script src="~/Scripts/js/source/html5.js"></script>
    <script src="~/Scripts/js/source/imagesloaded.pkgd.js"></script>
    <script src="~/Scripts/js/source/isotope.pkgd.js"></script>
    <script src="~/Scripts/js/source/jquery.flexslider.js"></script>
    <script src="~/Scripts/js/source/skip-link-focus-fix.js"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
